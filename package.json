  "dependencies": {
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.7",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bulma-extensions": "^6.2.7",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.14",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
